{
    "compilerOptions": {
        "sourceMap": true, 
        "target": "ES2017", 
        "outDir": "./dist", 
        "baseUrl": "./src", 
        "moduleResolution": "node", //Module 참조경로를 Class : 상대경로, node : node_modules 에서 참조플래그
        "experimentalDecorators": true,
    }, 
    "include": [
        "src/**/*.ts"
    ], 
    "exclude": [
        "node_modules"
    ]
}